<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".RoutingActivity$PlaceholderFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="UselessParent">

        <TextView
            android:id="@+id/settings_app_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/settings_app_name_padding_top"
            android:text="@string/app_name"
            android:textAlignment="center"
            android:textSize="@dimen/settings_app_name" />

        <TextView
            android:id="@+id/settings_app_name2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingVertical="@dimen/settings_developer_padding_vertical"
            android:text="@string/settings_developer_name"
            android:textAlignment="center"
            android:textSize="@dimen/settings_developer_name" />
    </LinearLayout>
</LinearLayout>
